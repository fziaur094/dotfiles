Host *
    UseKeychain yes # only works on macOS
    AddKeysToAgent yes

Host *.comcast.com *.comcast.net
    IdentityFile ~/.ssh/keys/fnu_ziaurrehman@comcast.com

Host github.com
    HostName ssh.github.com
    Port 443
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/keys/fnu_ziaurrehman@comcast.com
